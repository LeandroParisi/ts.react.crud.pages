    "classnames": "^2.3.1",
    "hamburger-react": "^2.4.1",
    "node-sass": "^4.14.1",
    "react": "^18.1.0",
    "react-bootstrap": "^2.0.3",
    "react-datepicker": "^4.2.1",
    "react-dom": "^18.1.0",
    "react-loader-spinner": "^4.0.0",
